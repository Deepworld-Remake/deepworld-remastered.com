{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
      "source": "**",
      "destination": "/index.html"
    }, {
      "source": "/faq",
      "destination": "/Pages/faq.html"
    }, {
      "source": "/codex",
      "destination": "/Pages/codex.html"
    }]
  }
}
